{
  "name": "@stockspro/sec-sdk",
  "version": "0.0.4",
  "description": "SDK for SEC data access",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "prepare": "npm run build",
    "generate": "openapi-generator-cli generate -i ../server/swagger.json -g typescript-axios -o ./src",
    "postgenerate": "npm run build",
    "preversion": "npm run build",
    "publish": "release-it",
    "publish:cicd": "release-it --ci",
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.4.1",
  "dependencies": {
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.16.3",
    "@types/node": "^22.13.5",
    "release-it": "^18.1.2",
    "release-it-pnpm": "^4.6.4",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@openapitools/openapi-generator-cli"
    ]
  },
  "files": [
    "dist"
  ]
}
